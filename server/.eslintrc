{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-restricted-exports": 0
  }
}
