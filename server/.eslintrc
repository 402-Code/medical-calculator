{
  "env": {
    "jest/globals": true,
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb", "eslint:recommended", "plugin:jest/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "arrow-body-style": 0,
    "no-shadow": 0
  }
}
